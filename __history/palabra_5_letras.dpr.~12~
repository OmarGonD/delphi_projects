program palabra_5_letras;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

var
    palabra: array[1..5] of char;
    letra:char;
    i, contador: byte;

begin
  Writeln('Escribe una palabra de 5 letras');
  for I := 1 to 5 do begin
    read(palabra[I]);
  end;
  readLn;


  for I := 1 to 5 do begin
    Writeln(palabra[I]);
  end;


  readln;

end.
